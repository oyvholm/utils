#!/usr/bin/env bash

#=======================================================================
# filmer
# File ID: b27679f4-5d39-11df-80fb-90e6ba3022ac
# Syntax: programnavn `find|filmer`
# Hehe, latterlig enkelt.
#=======================================================================

grep -Ei '\.(3gp|asx|avi|flv|m4v|mkv|mng|mov|mp4|mpeg|mpg|ogv|vob|webm|wmv)$' "$@"
