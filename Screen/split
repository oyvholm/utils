# split
# File ID: b785d050-7c7a-11e0-90c2-00023faf1383

select -
only
split
# cursor in upper win
resize 15
# top win is 15 high
split -v
# top win split vert, cursor in left
split
# top left win split hor, cursor at the top
split
# split upper left win, cursor top left
resize 3
# top left win is 3 high
select 21
focus
# cursor in small win below
resize 3
select 22
focus
# cursor in win23
split -v
# cursor in left empty window
select 23
focus
# cursor moved to the window on the right side
select 24
focus
# cursor upper right
select 25
focus
# cursor at the bottom
select 1

