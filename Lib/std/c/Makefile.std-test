# Makefile.std-test
# File ID: 0817d7ec-b768-11e6-a01b-279c2a0468a3

.PHONY: all
all:
	cd src && $(MAKE) -f Makefile.std-test

.PHONY: clean
clean:
	$(MAKE) -f Makefile clean
	cd src && $(MAKE) -f Makefile.std-test clean

.PHONY: test
test:
	rm -f README.html
	$(MAKE) README.html
	test -e README.html
	xmllint --noout README.html
	grep -q '^<p>Generated from <code>README.md</code>' README.html
	grep -q '^revision <code>' README.html
	$(MAKE) testsrc
	cd src && $(MAKE) -f Makefile.std-test test
