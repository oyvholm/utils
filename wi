search_str=
stdin=
if test -z "$1"; then
    stdin="`finduuid -u`"
fi
for f in "$@" $stdin; do
    search_str="$search_str -e $f"
done
grep -hi $search_str ~/src/git/uuids/*.xml ~/uuids/*.xml |
    sort -u |
    sortuuid |
    less
